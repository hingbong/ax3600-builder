echo "run rc.local"

sleep 50;
echo "------------------wifi down------------------"
ip link set down phy0-ap0;
ip link set down phy1-ap0;
sleep 10;
echo "------------------wifi up------------------"
ip link set up phy0-ap0;
ip link set up phy1-ap0;

exit 0

